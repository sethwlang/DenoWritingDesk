{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"]
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@1.0.0",
    "@vitejs/plugin-vue": "npm:@vitejs/plugin-vue@5.2.1",
    "sass-embedded": "npm:sass-embedded@^1.81.0",
    "sass-migrator": "npm:sass-migrator@2.2.1",
    "tailwindcss": "npm:tailwindcss@3.4.15",
    "vite": "npm:vite@6.0.1",
    "vue": "npm:vue@3.5.13",
    "vue-router": "npm:vue-router@4.5.0",
    "vue-slick-carousel": "npm:vue-slick-carousel@1.0.6"
  }
}
